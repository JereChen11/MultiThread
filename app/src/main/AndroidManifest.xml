<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.multithread">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".handler.HandlerObtainMessageActivity"></activity>
        <activity android:name=".handler.HandlerPostFunctionActivity" />
        <activity android:name=".multiAsyncTask.TestMultipleAsyncTask" />

        <service
            android:name=".intentservice.MyIntentService"
            android:exported="false" />

        <activity android:name=".intentservice.IntentServiceActivity" />
        <activity android:name=".asynctask.AsyncTaskActivity" />
        <activity android:name=".handlerthread.HandlerThreadActivity" />
        <activity android:name=".handler.HandlerAddThreadActivity" />
        <activity android:name=".ExtendThreadClass" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>